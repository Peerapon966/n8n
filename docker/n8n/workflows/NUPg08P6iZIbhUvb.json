    {
      "parameters": {
        "jsCode": "let errors = []\nlet input = {}\nfor (const field of $input.first().json.schemas) {\n  const key = field.name\n  const value = $input.first().json.values[field.name]\n\n  // set optional field with default and skip when value is empty (\"\", null, undefined)\n  if (field.optional && (value ?? \"\") === \"\") {\n    input[key] = field.default\n    continue;\n  } else {\n    input[key] = value\n  }\n  \n  if (typeof value != field.type.toLocaleLowerCase()) {\n    errors.push({\n      field: key,\n      issue: `Field '${key}' must be of type ${field.type}, ${typeof value} given.`})\n  }\n}\n\nreturn {\n  valid: errors.length == 0,\n  input,\n  errors\n}"
      },
